{
  "name": "vof-dev-tracker",
  "version": "1.0.0",
  "description": "An application to track VOF dev output per sprint",
  "main": "index.js",
  "repository": "https://github.com/Danielshow/vof-dev-tracker.git",
  "author": "Daniel Shotonwa <danielshotonwa53@gmail.com>",
